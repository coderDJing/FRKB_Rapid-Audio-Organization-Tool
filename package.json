{
  "name": "frkb",
  "version": "1.1.3-rc.202601310109",
  "description": "Rapid Audio Organization Tool",
  "license": "GPL-2.0-or-later",
  "main": "./out/main/index.js",
  "author": "CoderDJing",
  "homepage": "https://github.com/coderDJing/FRKB_Rapid-Audio-Organization-Tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/coderDJing/FRKB_Rapid-Audio-Organization-Tool.git"
  },
  "build": {
    "extraResources": [
      {
        "from": "vendor/ffmpeg",
        "to": "ffmpeg"
      },
      {
        "from": "vendor/chromaprint",
        "to": "chromaprint"
      }
    ],
    "appId": "com.electron.frkb",
    "productName": "FRKB",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "files": [
      "**/*",
      "!rust_package/target{,/**/*}",
      "!node_modules/rust_package/target{,/**/*}"
    ],
    "fileAssociations": [
      {
        "ext": [
          "mp3",
          "wav",
          "flac",
          "aif",
          "aiff",
          "ogg",
          "opus",
          "aac",
          "m4a",
          "mp4",
          "wma",
          "ac3",
          "dts",
          "mka",
          "webm",
          "ape",
          "tak",
          "tta",
          "wv"
        ],
        "name": "FRKB Audio",
        "description": "Audio files supported by FRKB",
        "role": "Editor"
      }
    ],
    "win": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "executableName": "FRKB"
    },
    "nsis": {
      "uninstallDisplayName": "FRKB",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true
    },
    "mac": {
      "x64ArchFiles": "Contents/Resources/{ffmpeg,chromaprint}/darwin/{ffmpeg,fpcalc}",
      "artifactName": "${productName}-${version}-universal.${ext}"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf dist",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run clean && npm run build && electron-builder --win",
    "build:mac": "npm run clean && npm run build && electron-builder --mac",
    "build:linux": "npm run clean && npm run build && electron-builder --linux",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "audio-decode": "^2.2.2",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.0",
    "electron-updater": "^6.3.9",
    "fs-extra": "^11.2.0",
    "hotkeys-js": "^3.13.7",
    "iconv-lite": "^0.6.3",
    "marked": "^17.0.0",
    "meyda": "^5.6.3",
    "mitt": "^3.0.1",
    "music-metadata": "^11.10.0",
    "overlayscrollbars": "^2.11.1",
    "overlayscrollbars-vue": "^0.5.9",
    "pinia": "^2.2.2",
    "rust_package": "./rust_package",
    "sass": "^1.79.3",
    "undici": "^6.19.8",
    "uuid": "^10.0.0",
    "vue-draggable-plus": "^0.5.3",
    "vue-i18n": "9"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "electron": "^38.7.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "vite": "^6.1.0",
    "vitepress": "^1.6.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,css,scss,less,json}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "electron",
      "esbuild",
      "vue-demi"
    ],
    "overrides": {
      "node-abi": "4.24.0"
    }
  }
}
